{
  "name": "redux-idle-callback",
  "version": "1.0.0",
  "description": "Middleware to detect when redux store has been idle",
  "main": "lib/index.js",
  "scripts": {
    "test:lint": "eslint --ignore-path .npmignore --ext js,jsx src __tests__",
    "test:unit": "jest",
    "test": "npm run test:unit && npm run test:lint",
    "test:lockfile": "lockfile-lint -p package-lock.json -t npm -a npm -o https: -c -i",
    "posttest": "npm run test:lockfile",
    "prepublish": "npm run build",
    "build": "babel src -d lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/americanexpress/redux-idle-callback.git"
  },
  "keywords": [
    "redux",
    "middleware",
    "idle"
  ],
  "author": "Andrew Sutedja <andrew.sutedja@aexp.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "amex-jest-preset": "^5.0.0",
    "babel-preset-amex": "^2.0.0",
    "eslint": "^5.5.0",
    "eslint-config-amex": "^8.0.1",
    "jest": "^24.1.0",
    "lockfile-lint": "^3.0.8"
  },
  "jest": {
    "preset": "amex-jest-preset",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {}
}
